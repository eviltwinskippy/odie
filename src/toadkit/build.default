###
# This file is source by scripts/configure.tcl and defines all of the defaults
# assumed by the odiekit
###
set kitsrcroot [file dirname [file normalize [info script]]]

set baseKitPath ~/odie/bin/
set developer_root ~/Developer

set tcl_major_version 8
set tcl_minor_version 6

set ODIEVERSION 2.0
set TARGET [lindex $tcl_platform(os) 0]
set HOST [lindex $tcl_platform(os) 0]